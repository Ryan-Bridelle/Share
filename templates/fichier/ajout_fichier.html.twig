{% extends 'base.html.twig' %}
 
{% block title %}
    {{parent()}}
     Ajouter Fichier
{% endblock %}
 
{% block body %}
    {{parent()}}
    <div class="container">
        <div class="row justify-content-center">
            <h1 class="text-center text-primary p-4">Ajouter un fichier
            </h1>
        </div>
        <div class="row justify-content-center">
            <div class="col-12 bg-white p-4 m-0 text-primary">
                {{ form_start(form) }}
                <div class="form-group">
                    {{ form_label(form.nom, 'Nom ', {'label_attr': {'class': 'font-weight-bold'}}) }}
                    {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
                </div>
     
        </div>
    </div>
 
    {{form_end(form) }}
 
 
{% endblock %}
    